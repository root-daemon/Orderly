generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String  @id @unique @default(cuid())
  email           String  @unique
  accessToken     String?
  refreshToken    String?
  timetable       Json?
  academiaCookies Json?
  academiaEmail   String?
  enabled         Boolean
}

model Academia {
  id       String @id @unique @default(cuid())
  date     String @unique
  dayOrder Int
}
